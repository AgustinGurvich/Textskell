

C
  : Comm ';'

Comm
  : Var '<-' Atom
  | setPlayer Player
  | setCell '(' Int ',' Int ')' Cell
  | setMap File

Cell
  : Empty
  | Treasure '(' Atom ',' Atom ')'
  | Enemy '(' Atom ',' Atom ')'
  | Exit

Atom
  : '(' Int ',' Int ')'
  | '(' Atom ',' Buff ',' Int ')'
  | Var

Player
  : '(' Int ',' Int ',' '(' Int ',' Int ')' ')'

Buff
  : Dmg
  | HP

File
  : File